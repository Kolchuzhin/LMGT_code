*cfwrite,/nopr
:nl1
*get,ele,node,,num,min
*if,ele,eq,0,:nl2
radi=NX(ele)
!Abstand*****************************************
xm=lg
abst_x=radi+xm
abst_x=abst_x*1000
abst_y=abst_x**2*x0+abst_x*x1+x2
abst_y=1/(-abst_y)
abst_y=abst_y+x3
abst_y=abst_y/1000000
abst=abst_y
*if,abst,ge,df,:nl5
!abst=df
:nl5
*cfwrite,d,ele,uy,abst                           !Ventil an den Anschlag druecken
nsel,u,node,,ele
*go,:nl1
:nl2
*cfwrite,/gopr
*cfclose,updruck1
/gopr
*end


